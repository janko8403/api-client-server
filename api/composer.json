{
  "name": "laminas/laminas-mvc-skeleton",
  "description": "Laminas MVC Skeleton Application",
  "type": "project",
  "license": "BSD-3-Clause",
  "keywords": [
    "laminas",
    "mvc",
    "skeleton",
    "framework"
  ],
  "require": {
    "php": "^8.0",
    "ext-intl": "*",
    "beberlei/doctrineextensions": "^1.3",
    "doctrine/doctrine-orm-module": "^5.1",
    "guzzlehttp/psr7": "^1",
    "laminas-api-tools/api-tools": "~1.0",
    "laminas-api-tools/api-tools-oauth2": "^1.7",
    "laminas/laminas-authentication": "^2.10",
    "laminas/laminas-cache": "^3.1",
    "laminas/laminas-cache-storage-adapter-filesystem": "^2.0",
    "laminas/laminas-cache-storage-adapter-memory": "^2.0",
    "laminas/laminas-component-installer": "^2.4",
    "laminas/laminas-db": "^2.12.0",
    "laminas/laminas-development-mode": "^3.2",
    "laminas/laminas-i18n-resources": "^2.8",
    "laminas/laminas-json": "^3.2",
    "laminas/laminas-log": "^2.15",
    "laminas/laminas-mvc": "^3.1.1",
    "laminas/laminas-mvc-form": "^2.0.0",
    "laminas/laminas-mvc-i18n": "^1.2.0",
    "laminas/laminas-mvc-plugins": "^1.1.0",
    "laminas/laminas-navigation": "^2.13",
    "laminas/laminas-permissions-acl": "^2.9",
    "laminas/laminas-serializer": "^2.12",
    "laminas/laminas-session": "^2.10.0",
    "laminas/laminas-skeleton-installer": "^0.3",
    "lm-commons/lmc-cors": "^1.2",
    "mailgun/mailgun-php": "^3.5",
    "mobiledetect/mobiledetectlib": "^2.8",
    "mpdf/mpdf": "^8.1.1",
    "neilime/twbs-helper-module": "^5.1",
    "nyholm/psr7": "^1.5",
    "rnd-cosoft/slm-queue-rabbitmq": "^3.0",
    "rollbar/rollbar": "^3.1",
    "slm/queue": "^3.1",
    "smsapi/php-client": "^3.0",
    "symfony/http-client": "^6.0",
    "symfony/yaml": "^6.0",
    "vlucas/phpdotenv": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "Application\\": "module/Application/src/",
      "Hr\\": "module/Hr/src/",
      "Users\\": "module/Users/src/",
      "Customers\\": "module/Customers/src/",
      "DocumentTemplates\\": "module/DocumentTemplates/src/",
      "Configuration\\": "module/Configuration/src/",
      "Settings\\": "module/Settings/src/",
      "Notifications\\": "module/Notifications/src/",
      "AssemblyOrders\\": "module/AssemblyOrders/src/",
      "NpsRating\\": "module/NpsRating/src/",
      "ApiAssemblyOrders\\": "module/ApiAssemblyOrders/src/",
      "ApiNpsRatings\\": "module/ApiNpsRatings/src/",
      "ApiUsers\\": "module/ApiUsers/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ApplicationTest\\": "module/Application/test/"
    }
  },
  "scripts": {
    "cs-check": "vendor/bin/phpcs",
    "cs-fix": "vendor/bin/phpcbf",
    "development-disable": "laminas-development-mode disable",
    "development-enable": "laminas-development-mode enable",
    "development-status": "laminas-development-mode status",
    "post-create-project-cmd": [
      "@development-enable",
      "php bin/update-gitignore.php",
      "php -r 'if (file_exists(\"bin/remove-package-artifacts.php\")) include \"bin/remove-package-artifacts.php\";'",
      "php -r 'if (file_exists(\"CHANGELOG.md\")) unlink(\"CHANGELOG.md\");'"
    ],
    "serve": "php -S 0.0.0.0:8080 -t public",
    "test": "vendor/bin/phpunit",
    "static-analysis": "vendor/bin/psalm --shepherd --stats",
    "post-install-cmd": "cd public && npm install && npm run build"
  },
  "scripts-descriptions": {
    "cs-check": "Run coding standards checks.",
    "cs-fix": "Automatically fix coding standard issues.",
    "development-disable": "Disable development mode.",
    "development-enable": "Enable development mode.",
    "development-status": "Detail whether or not the application is in development mode.",
    "serve": "Start the built-in PHP web server and serve the application.",
    "test": "Run unit tests."
  },
  "require-dev": {
    "laminas-api-tools/api-tools-admin": "~1.0",
    "laminas-api-tools/api-tools-asset-manager": "^1.5",
    "laminas/laminas-developer-tools": "^2.1.1",
    "symfony/var-dumper": "^6.0"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "laminas/laminas-component-installer": true,
      "laminas-api-tools/api-tools-asset-manager": true,
      "laminas/laminas-skeleton-installer": true
    }
  }
}
